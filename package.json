{
	"name": "directus-extension-calculated-time-series-panel",
	"description": "This is a Directus extension providing a flexible panel that can display a time series chart based on calculated values with support for grouping and filtering.",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-panel"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "panel",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.1.1",
		"typescript": "^5.8.3",
		"vue": "^3.5.17"
	},
	"dependencies": {
		"apexcharts": "^4.7.0",
		"date-fns": "^4.1.0",
		"lodash": "^4.17.21",
		"lodash-es": "^4.17.21",
		"mathjs": "^14.5.2"
	}
}
